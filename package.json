{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-dev": "cross-env NODE_ENV=\"development\" vue-cli-service serve --mode dev",
    "serve-test": "cross-env NODE_ENV=\"development\" vue-cli-service serve --mode test",
    "serve-prod": "cross-env NODE_ENV=\"development\" vue-cli-service serve --mode prod",
    "build-dev": "cross-env NODE_ENV=\"production\" vue-cli-service build --mode dev --modern",
    "build-test": "cross-env NODE_ENV=\"production\" vue-cli-service build --mode test --modern",
    "build-prod": "cross-env NODE_ENV=\"production\" vue-cli-service build --mode prod --modern",
    "start": "cross-env NODE_ENV=\"development\" vue-cli-service serve --mode dev",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.8",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "qs": "^6.7.0",
    "superagent": "^5.1.0",
    "vant": "^2.0.3",
    "vconsole": "^3.3.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "postcss-pxtorem": "^4.0.1",
    "vue-template-compiler": "^2.6.10"
  }
